{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
  	"no-console": false,
    "prefer-const": false,
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
